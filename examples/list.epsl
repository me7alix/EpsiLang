nums := [1, 2];
println(kind(nums));

append(nums, 3, 4, 5);
println(nums);

nums[1] = 777;
println(nums);

remove(nums, 1);
println(nums);

insert(nums, 0, 101);
println(nums);

print("{ ");
for n in nums ->
	print(n, " ");
println("}");

fn check(nums, num) {
	println("has ", num, ": ", has(nums, num));
}

check(nums, 3);
check(nums, 0);
check(nums, 101);
