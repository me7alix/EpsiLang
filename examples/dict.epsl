dict := {
	"zero":  0,
	"one":   0,
	"two":   2,
	"three": 3,
	"four":  4,
	"five":  5,
	"six":   6,
	"seven": 7,
	"eight": 8,
};

dict["nine"] = 9;
dict["one"] = 1;

println(dict);

while true {
	key := input("enter digit key: ");
	if has(dict, key) -> println("digit: ", str(dict[key]));
	else              -> println("no such digit");
}
